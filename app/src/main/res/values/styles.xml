<resources>
    <!-- Light Theme -->
    <style name="AppTheme.Light" parent="Theme.AppCompat.Light">
        <!-- Primary brand color -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>

        <!-- Secondary brand color -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>

        <!-- Status bar color -->
        <item name="android:statusBarColor">@color/parchment_dark_status_bar</item>
    </style>

    <!-- Dark Theme -->
    <style name="AppTheme.Discord" parent="Theme.AppCompat">
        <item name="colorPrimary">#7289DA</item>
        <item name="colorPrimaryVariant">#424549</item>
        <item name="colorOnPrimary">#FFFFFF</item>

        <!-- Secondary brand color -->
        <item name="colorSecondary">#36393E</item>
        <item name="colorSecondaryVariant">#282B30</item>
        <item name="colorOnSecondary">#FFFFFF</item>

        <!-- Status bar color -->
        <item name="android:statusBarColor">#1E2124</item>
    </style>



</resources>
